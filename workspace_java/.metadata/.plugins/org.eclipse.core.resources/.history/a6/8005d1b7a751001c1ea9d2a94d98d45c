package operator;

public class Ex1 {
	public static void main(String[] args) {
		
		/*
		 * 단항 연산자(증감 연산자) (++, --)
		 * 
		 *  1. 선행연산자
		 *  
		 *  2. 후행연산자
		 * 
		 * */
		
		int i = 3;
		i++;
		System.out.println(i);
		++i;
		System.out.println(i);
		System.out.println(++i);
		System.out.println(i++);
		
		
		
		
	}
}
