package operator;

public class Ex2 {

	public static void main(String[] args) {

		/*
		 * 산술연산자 (+, -, *, /, %)
		 * - 일반적인 사칙연산과 동일
		 * - % 연산자: 나머지 연산자(퍼센트 연산자)라고 하며, 나눈 나머지 값을 반환
		 * 
		 * */
		
		System.out.println(10+2);
		System.out.println(10-2);
		System.out.println(10*2);
		System.out.println(10/2);
		
	}

}
