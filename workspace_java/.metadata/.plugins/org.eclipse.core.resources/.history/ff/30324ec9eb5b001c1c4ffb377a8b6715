package method;

public class Test1 {

	public static void main(String[] args) {

		// 1. 매개변수도 없고, 리턴값도 없는 메서드 호출
		// "Hellow, World!" 문자열 10번 반복 출력하는 hello() 메서드를 호출
		hello();
	}
	
	public static void hello() {
		for(int i=0; i<10; i++) {
			System.out.println(i + ": Hellow, World!");
		}
	}
	
	

}
