package typecasting;

public class Ex1 {

	public static void main(String[] args) {

		// byte, short, char 끼리 형변환
		/*
		byte b = 127;
		char c = (char)b;
		System.out.println(c);
		
		short s = 10000;
		c = (char)s;
		System.out.println(c);
		*/
		
		int a = 3;
		float b = 1.0F;
		double c = a + b;
		System.out.println(c);
		
		float f = 100.0;
		
		
		
	}

}
