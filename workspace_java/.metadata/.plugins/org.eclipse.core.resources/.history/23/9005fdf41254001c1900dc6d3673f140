package switch_case;

public class Test3 {

	public static void main(String[] args) {
		
		// 세 정수 n1, n2, n3 중 최대값을 출력
		int n1=1, n2=3, n3=5;
		
		if(n1 > n2) {
			
			if(n1 > n3) {
				System.out.println("최대값 : " + n1);
			} else {
				System.out.println("최대값 : " + n3);
			}
			
		}
		
		
		
		
		
		
		
	}

}
