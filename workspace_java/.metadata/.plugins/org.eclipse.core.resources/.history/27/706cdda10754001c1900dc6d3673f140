package switch_case;

public class Test1 {

	public static void main(String[] args) {

		// 학생 점수 (score)에 대한 학점(grade) 판별
		// A학점: 90 ~ 100점
		// B학점: 80 ~ 89점
		// C학점: 70 ~ 79점
		// D학점: 60 ~ 69점
		// F학점: 0  ~ 59점
		// 그 외: "점수입력 오류!"
		int score = 90;
		String grade = "";
		switch (score/10) {
			case 10:
				break;
			case 9:
				break;
			case 8:
				break;
			case 7:
				break;
			case 6:
				break;
			case 5:
				break;
			case 4:
				break;
			case 3:
				break;
			case 2:
				break;
			case 1:
				break;
			case 0:
				break;
			default:
				grade = "점수입력 오류!";
				break;
		}
		
		
		
		
		
		
	}

}
